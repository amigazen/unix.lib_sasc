DEST = /SDK/lib/

RM = delete QUIET
CP = copy CLONE QUIET
CC = sc
AR = oml
AS = asm

OBJS =	_exit.o _main.o abort.o access.o alloca.o amigados.o amigatime.o \
	bcmp.o bcopy.o bzero.o chmod.o chown.o close.o creat.o ctime.o \
	devices.o dir.o dir_data.o dup.o endpwent.o error.o error_msg.o \
	exec.o fchmod.o fcntl.o fd_setsize.o fibstat.o fifofd.o files.o \
	fnmatch.o fstat.o ftruncate.o getenv.o gethostname.o getopt.o \
	getopt1.o getpid.o getwd.o gid.o grp.o hypot.o index.o ioctl.o \
	isatty.o kill.o link.o lseek.o lstat.o message.o mkdir.o mkfifo.o \
	mktemp.o onbreak.o open.o perror.o pipe.o popen.o processes.o \
	protection.o pseudo_c.o pwd.o raise.o read.o regerror.o regex.o \
	regexp.o regsub.o rename.o rindex.o rmdir.o select.o setenv.o \
	setjmp.o siglist.o signal.o signals.o sigpause.o sigprocmask.o \
	sigsetmask.o sleep.o sockets.o snprintf.o stat.o strcasecmp.o \
	strdup.o strerror.o strftime.o strtol.o strtoll.o longlong.o symlink.o \
	tempnam.o time.o timeconvert.o timers.o truncate.o uid.o umask.o \
	unlink.o usleep.o utime.o vsnprintf.o wait4.o write.o

DOBJS =	_exit.do _main.do abort.do access.do alloca.do amigados.do \
	amigatime.do bcmp.do bcopy.do bzero.do chmod.do chown.do close.do \
	creat.do ctime.do devices.do dir.do dir_data.do dup.do endpwent.do \
	error.do error_msg.do exec.do fchmod.do fcntl.do fd_setsize.do \
	fibstat.do fifofd.do files.do fnmatch.do fstat.do ftruncate.do \
	getenv.do gethostname.do getopt.do getopt1.do getpid.do getwd.do \
	gid.do grp.do hypot.do index.do ioctl.do isatty.do kill.do link.do \
	lseek.do lstat.do message.do mkdir.do mkfifo.do mktemp.do \
	onbreak.do open.do perror.do pipe.do popen.do processes.do \
	protection.do pseudo_c.do pwd.do raise.do read.do regerror.do \
	regex.do regexp.do regsub.do rename.do rindex.do rmdir.do select.do \
	setenv.do setjmp.do siglist.do signal.do signals.do sigpause.do \
	sigprocmask.do sigsetmask.do sleep.do sockets.do snprintf.do stat.do \
	strcasecmp.do strdup.do strerror.do strftime.do strtol.do strtoll.do longlong.do \
	symlink.do tempnam.do time.do timeconvert.do timers.do truncate.do \
	uid.do umask.do unlink.do usleep.do utime.do vsnprintf.do wait4.do write.do

NBOBJS=	_exit.nbo _main.nbo abort.nbo access.nbo alloca.nbo amigados.nbo \
	amigatime.nbo bcmp.nbo bcopy.nbo bzero.nbo chmod.nbo chown.nbo \
	close.nbo creat.nbo ctime.nbo devices.nbo dir.nbo dir_data.nbo \
	dup.nbo endpwent.nbo error.nbo error_msg.nbo exec.nbo fchmod.nbo \
	fcntl.nbo fd_setsize.nbo fibstat.nbo fifofd.nbo files.nbo \
	fnmatch.nbo fstat.nbo ftruncate.nbo getenv.nbo gethostname.nbo \
	getopt.nbo getopt1.nbo getpid.nbo getwd.nbo gid.nbo grp.nbo \
	hypot.nbo index.nbo ioctl.nbo isatty.nbo kill.nbo link.nbo \
	lseek.nbo lstat.nbo message.nbo mkdir.nbo mkfifo.nbo mktemp.nbo \
	onbreak.nbo open.nbo perror.nbo pipe.nbo popen.nbo processes.nbo \
	protection.nbo pseudo_c.nbo pwd.nbo raise.nbo read.nbo regerror.nbo \
	regex.nbo regexp.nbo regsub.nbo rename.nbo rindex.nbo rmdir.nbo \
	select.nbo setenv.nbo setjmp.nbo siglist.nbo signal.nbo signals.nbo \
	sigpause.nbo sigprocmask.nbo sigsetmask.nbo sleep.nbo sockets.nbo \
	snprintf.nbo stat.nbo strcasecmp.nbo strdup.nbo strerror.nbo strftime.nbo \
	strtol.nbo strtoll.nbo longlong.nbo symlink.nbo tempnam.nbo time.nbo \
	timeconvert.nbo timers.nbo truncate.nbo uid.nbo umask.nbo unlink.nbo \
	usleep.nbo utime.nbo vsnprintf.nbo wait4.nbo write.nbo

DNBOBJS=_exit.dnbo _main.dnbo abort.dnbo access.dnbo alloca.dnbo \
	amigados.dnbo amigatime.dnbo bcmp.dnbo bcopy.dnbo bzero.dnbo \
	chmod.dnbo chown.dnbo close.dnbo creat.dnbo ctime.dnbo devices.dnbo \
	dir.dnbo dir_data.dnbo dup.dnbo endpwent.dnbo error.dnbo \
	error_msg.dnbo exec.dnbo fchmod.dnbo fcntl.dnbo fd_setsize.dnbo \
	fibstat.dnbo fifofd.dnbo files.dnbo fnmatch.dnbo fstat.dnbo \
	ftruncate.dnbo getenv.dnbo gethostname.dnbo getopt.dnbo \
	getopt1.dnbo getpid.dnbo getwd.dnbo gid.dnbo grp.dnbo hypot.dnbo \
	index.dnbo ioctl.dnbo isatty.dnbo kill.dnbo link.dnbo lseek.dnbo \
	lstat.dnbo message.dnbo mkdir.dnbo mkfifo.dnbo mktemp.dnbo \
	onbreak.dnbo open.dnbo perror.dnbo pipe.dnbo popen.dnbo \
	processes.dnbo protection.dnbo pseudo_c.dnbo pwd.dnbo raise.dnbo \
	read.dnbo regerror.dnbo regex.dnbo regexp.dnbo regsub.dnbo \
	rename.dnbo rindex.dnbo rmdir.dnbo select.dnbo setenv.dnbo \
	setjmp.dnbo siglist.dnbo signal.dnbo signals.dnbo sigpause.dnbo \
	sigprocmask.dnbo sigsetmask.dnbo sleep.dnbo sockets.dnbo snprintf.dnbo \
	stat.dnbo strcasecmp.dnbo strdup.dnbo strerror.dnbo strftime.dnbo strtol.dnbo \
	strtoll.dnbo longlong.dnbo symlink.dnbo tempnam.dnbo time.dnbo \
	timeconvert.dnbo timers.dnbo truncate.dnbo uid.dnbo umask.dnbo \
	unlink.dnbo usleep.dnbo utime.dnbo vsnprintf.dnbo wait4.dnbo write.dnbo

ASMOBJS = child.o _sprintf.o _setjmp.o

ALLOBJS = $(OBJS) $(ASMOBJS)
ALLNBOBJS = $(NBOBJS) $(ASMOBJS)
ALLDOBJS = $(DOBJS) $(ASMOBJS)
ALLDNBOBJS = $(DNBOBJS) $(ASMOBJS)

DEFS = DEF AMITCP=1 DEF NO_MACROS=1 DEF __USE_SYSBASE=1

IDIRS = IDIR=include IDIR=include/internal IDIR=include: IDIR=netinclude: 

SCOPTIONS = NOVER NOICONS CPU=ANY PARMS=BOTH NOSTKCHK IDLEN=100 \
	    AFP MCCONS STREQ UTILLIB MATH=IEEE OPTTIME OPTSCHED \
	    $(DEFS) $(IDIRS) IGNORE=74,92,101,104,306,317

OFLAGS = OPTIMIZE
DFLAGS = NOOPTIMIZE DEBUG=SF

CFLAGS =    GST=amiga.gst DATA=NEAR $(IDIRS)
CFLAGS_NB = GST=amiganb.gst DATA=FAR $(IDIRS)

LIB = unix.lib
FARLIB = unixnb.lib
DLIB = unixd.lib
DFARLIB = unixdnb.lib

.SUFFIXES: .c .a .o .do .nbo .dnbo
#
# Generic rules
#
.c.o:
	$(CC) $(CFLAGS) $(OFLAGS) $*.c
.c.do:
	$(CC) $(CFLAGS) $(DFLAGS) $*.c OBJNAME=$*.do
.c.nbo:
	$(CC) $(CFLAGS_NB) $(OFLAGS) $*.c OBJNAME=$*.nbo
.c.dnbo:
	$(CC) $(CFLAGS_NB) $(DFLAGS) $*.c OBJNAME=$*.dnbo
.a.o:
	$(AS) -o$*.o -iINCLUDE: $*.a

normal: SCOPTIONS amiga.gst $(LIB)

all: normal amiganb.gst $(FARLIB)

debug: SCOPTIONS amiga.gst $(DLIB) amiganb.gst $(DFARLIB)

install: normal
	$(CP) $(LIB) $(DEST)

install-all: all install
	$(CP) $(FARLIB) $(DEST)

install-debug: debug
	$(CP) $(DLIB) $(DFARLIB) $(DEST)

clean:
	-$(RM) \#?.(lib|o|do|nbo|dnbo|gst|lst|map) SCOPTIONS

SCOPTIONS: SMakefile
	copy to $@ <from <
$(SCOPTIONS)
<

#
# Normal libraries
#
$(LIB): SCOPTIONS $(ALLOBJS)
	-$(RM) $(LIB) ram:$(LIB)
	$(AR) ram:$(LIB) r <@<
$(ALLOBJS)
<
	$(CP) ram:$(LIB) ""
	$(RM) ram:$(LIB)

$(FARLIB): SCOPTIONS $(ALLNBOBJS)
	-$(RM) $(FARLIB) ram:$(FARLIB)
	$(AR) ram:$(FARLIB) r <@<
$(ALLNBOBJS)
<
	$(CP) ram:$(FARLIB) ""
	$(RM) ram:$(FARLIB)

#
# Debugging libraries
#
$(DLIB): SCOPTIONS $(ALLDOBJS)
	-$(RM) $(DLIB) ram:$(DLIB)
	$(AR) ram:$(DLIB) r <@<
$(ALLDOBJS)
<
	$(CP) ram:$(DLIB) ""
	$(RM) ram:$(DLIB)

$(DFARLIB): SCOPTIONS $(ALLDNBOBJS)
	-$(RM) $(DFARLIB) ram:$(DFARLIB)
	$(AR) ram:$(DFARLIB) r <@<
$(ALLDNBOBJS)
<
	$(CP) ram:$(DFARLIB) ""
	$(RM) ram:$(DFARLIB)

#
# Specific rules
#
hypot.o: hypot.c
	$(CC) $(CFLAGS) hypot.c

hypot.nbo: hypot.c
	$(CC) $(CFLAGS_NB) hypot.c OBJNAME=hypot.nbo

setjmp.do: setjmp.c signals.h amiga.h
	sc $(CFLAGS) setjmp.c OBJNAME=setjmp.do

setjmp.dnbo: setjmp.c signals.h amiga.h
	sc $(CFLAGS_NB) setjmp.c OBJNAME=setjmp.dnbo

amiga.gst: amiga.h
	$(CP) amiga.h am.c
	$(CC) NOOBJNAME DATA=NEAR MAKEGST=amiga.gst am.c
	$(RM) am.c

amiganb.gst: amiga.h
	$(CP) amiga.h am.c
	$(CC) NOOBJNAME DATA=FAR MAKEGST=amiganb.gst am.c
	$(RM) am.c

